
public class q1 {
		
		

		public static void main(String[] args) {
			
			int arr[]={5,1,7,4,9,3};
			//int count =0;
			int x=1;
			//bubbleSort(arr);
			//int n=biSearch(arr,x);
			q1 q=new q1();
			q.bubbleSort(arr);
			//q.print(arr);
			int n=q.biSearch(arr, x);
			
			
			System.out.println(n);
			//System.out.println();
			
			
			


		}
		public  void bubbleSort(int[]arr)
		{
			int n=arr.length;
			for(int i=0;i<n;i++)
			{
				for(int j=1;j<n;j++)
				{
					if(arr[j-1]>arr[j])
					{
						int temp=arr[j];
						 arr[j]=arr[j-1];
						 arr[j-1]=temp;
						
					}
				}
			}
		}
		 public  void print(int [] arr) {
			int n=arr.length;
			for(int i=0;i<n;i++) {
				System.out.println(arr[i]+" ");
			}
			
		}
		
		
		
		
		
	public int  biSearch(int[] arr,int x) {
			
			int start=0;
			int end=arr.length-1;
			
			int mid=start+(end-start)/2;
			
			while(start<=end){
				if(arr[mid] == x)
				{
					//System.out.println(mid); 
					return mid;
					}
				
				if(arr[mid]<x)
					start=mid+1;
				else
					end=mid-1;
				
			//count++;
				
			}
			//System.out.println(count);
			return -1; 
			
		}
}


